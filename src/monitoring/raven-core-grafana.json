{
    "__inputs": [
        {
            "name": "DS_PROMETHEUS",
            "label": "Prometheus",
            "description": "Prometheus data source",
            "type": "datasource",
            "pluginId": "prometheus",
            "pluginName": "Prometheus"
        }
    ],
    "__requires": [
        {
            "type": "grafana",
            "id": "grafana",
            "name": "Grafana",
            "version": "9.5.0"
        },
        {
            "type": "panel",
            "id": "stat",
            "name": "Stat",
            "version": ""
        },
        {
            "type": "panel",
            "id": "timeseries",
            "name": "Time series",
            "version": ""
        },
        {
            "type": "panel",
            "id": "table",
            "name": "Table",
            "version": ""
        },
        {
            "type": "datasource",
            "id": "prometheus",
            "name": "Prometheus",
            "version": "1.0.0"
        }
    ],
    "annotations": {
        "list": [
            {
                "builtIn": 1,
                "datasource": "-- Grafana --",
                "enable": true,
                "hide": true,
                "iconColor": "rgba(0, 211, 255, 1)",
                "name": "Annotations & Alerts",
                "type": "dashboard"
            }
        ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "id": null,
    "iteration": 1,
    "links": [],
    "panels": [
        {
            "type": "stat",
            "title": "Requests /s",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 6,
                "w": 6,
                "x": 0,
                "y": 0
            },
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "orientation": "auto",
                "colorMode": "value",
                "graphMode": "none",
                "justifyMode": "auto"
            },
            "targets": [
                {
                    "expr": "sum(rate(http_request_duration_seconds_count[5m]))",
                    "legendFormat": "req/s",
                    "refId": "A"
                }
            ]
        },
        {
            "type": "stat",
            "title": "5xx Error Rate (%)",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 6,
                "w": 6,
                "x": 6,
                "y": 0
            },
            "options": {
                "reduceOptions": {
                    "calcs": [
                        "lastNotNull"
                    ],
                    "fields": "",
                    "values": false
                },
                "colorMode": "value",
                "graphMode": "none",
                "justifyMode": "auto"
            },
            "targets": [
                {
                    "expr": "100 * ( sum(rate(http_request_duration_seconds_count{status=~\"5..\"}[5m])) / clamp_min(sum(rate(http_request_duration_seconds_count[5m])), 1e-9) )",
                    "legendFormat": "5xx %",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent"
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Latency (p50 / p95 / p99) — global",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom"
                }
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p50",
                    "refId": "A"
                },
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p95",
                    "refId": "B"
                },
                {
                    "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
                    "legendFormat": "p99",
                    "refId": "C"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Requests by status",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 6
            },
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom"
                },
                "stacking": {
                    "mode": "normal",
                    "group": "A"
                }
            },
            "targets": [
                {
                    "expr": "sum by (status) (rate(http_request_duration_seconds_count[5m]))",
                    "legendFormat": "{{status}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s"
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "Latency p95 by route",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 8
            },
            "options": {
                "legend": {
                    "displayMode": "table",
                    "placement": "bottom"
                }
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
                    "legendFormat": "{{route}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                },
                "overrides": []
            }
        },
        {
            "type": "table",
            "title": "Top 10 slowest routes (p95, last 15m)",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 14
            },
            "targets": [
                {
                    "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[15m])) by (le, route)))",
                    "legendFormat": "{{route}}",
                    "refId": "A",
                    "format": "table",
                    "instant": true
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                },
                "overrides": []
            },
            "options": {
                "showHeader": true
            }
        },
        {
            "type": "timeseries",
            "title": "Process memory (RSS)",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 16
            },
            "targets": [
                {
                    "expr": "raven_core_process_resident_memory_bytes",
                    "legendFormat": "rss",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "bytes"
                },
                "overrides": []
            }
        },
        {
            "type": "timeseries",
            "title": "CPU (user+system)",
            "datasource": "${DS_PROMETHEUS}",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 22
            },
            "targets": [
                {
                    "expr": "rate(raven_core_process_cpu_user_seconds_total[5m]) + rate(raven_core_process_cpu_system_seconds_total[5m])",
                    "legendFormat": "CPU seconds/s",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s/s"
                },
                "overrides": []
            }
        }
    ],
    "refresh": "10s",
    "schemaVersion": 38,
    "style": "dark",
    "tags": [
        "raven-core",
        "node",
        "prometheus"
    ],
    "templating": {
        "list": []
    },
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "timepicker": {},
    "timezone": "",
    "title": "Raven-core — API Overview",
    "uid": null,
    "version": 1,
    "weekStart": ""
}