{
    "version": 2,
    "functions": {
        "api/**/*.ts": {
            "runtime": "nodejs20.x"
        },
        "api/index.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        }
    ]
}