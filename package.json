    "@aws-sdk/lib-dynamodb": "^3.888.0",
    "@prisma/client": "^6.16.1",
    "@sendgrid/mail": "^8.1.6",
    "@upstash/redis": "^1.35.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.7.8",
    "chrono-node": "^2.6.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.23",
    "openai": "^5.22.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
