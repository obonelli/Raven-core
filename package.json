{
  "name": "my-api",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\" --cache --cache-location node_modules/.cache/eslint",
    "dev": "nodemon -q --watch src --ext ts,tsx,js,json --exec \"tsx src/server.ts\"",
    "build": "tsc -p tsconfig.json",
    "start:api": "node dist/server.js",
    "start:worker": "node dist/workers/notify.worker.js",
    "start:all": "bash -lc 'node dist/server.js & p1=$!; node dist/workers/notify.worker.js & p2=$!; wait -n $p1 $p2; exit $?'",
    "start": "node dist/server.js",
    "test": "jest --runInBand --config jest.config.ts",
    "seed:dev:user": "tsx scripts/seed-dev-user.ts",
    "test:email": "tsx scripts/test-email.ts",
    "worker:notify:dev": "tsx src/workers/notify.worker.ts",
    "worker:notify": "node dist/workers/notify.worker.js",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.888.0",
