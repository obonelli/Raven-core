    "@types/express": "^5.0.3",
    "@types/express-slow-down": "^2.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.5.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jest": "29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
